{
  "name": "svelte-adapter-github-pages",
  "description": "SvelteKit adapter for GitHub Pages.",
  "author": "iivusly <52052910+iivusly@users.noreply.github.com>",
  "version": "0.1.0",
  "main": "build/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "format": "npm run check-format -- --write",
    "check-format": "prettier --check .",
    "check-types": "tsc --skipLibCheck",
    "test": "uvu test test.js"
  },
  "exports": {
    ".": {
      "types": "./build/index.d.ts",
      "import": "./build/index.js"
    },
    "./package.json": "./package.json"
  },
  "license": "MIT",
  "keywords": [
    "svelte",
    "sveltekit",
    "sveltekit-adapter",
    "github-pages",
    "staticwebapp"
  ],
  "homepage": "https://github.com/iivusly/svelte-adapter-github-pages#readme",
  "bugs": {
    "url": "https://github.com/iivusly/svelte-adapter-github-pages/issues"
  },
  "peerDependencies": {
    "@sveltejs/kit": "^1.0.0"
  },
  "devDependencies": {
    "@sveltejs/kit": "^1.8.8",
    "@types/node": "^17.0.5",
    "playwright": "^1.33.0",
    "prettier": "^2.4.1",
    "svelte": "^3.58.0",
    "typescript": "^4.8.4",
    "uvu": "^0.5.6",
    "vite": "^4.3.3"
  },
  "files": [
    "index.js",
    "index.d.ts"
  ]
}
